{
  "name": "address-data-api",
  "version": "1.0.0",
  "description": "An Api that allows querying various services that return useful data about an ipaddress or domain",
  "main": "app.js",
  "scripts": {
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"yarn run watch-ts\" \"yarn run watch-node\"",
    "prettier": "prettier --write \"**/*.ts\"",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "lint": "tslint --project tsconfig.json"
  },
  "author": "Robert McCardell",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^3.5.1",
    "config": "^3.0.0",
    "ip-address": "^5.8.9",
    "poolio": "^0.2.54018",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "threads": "^0.12.0",
    "webworker-threads": "^0.7.17",
    "worker-thread": "^1.1.0"
  },
  "devDependencies": {
    "@sgarciac/bombadil": "^2.0.1",
    "@swimlane/prettier-config-swimlane": "^0.1.1",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^3.0.5",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "assign-deep": "^1.0.0",
    "await-fs": "^1.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "esm": "^3.0.84",
    "express": "^4.16.4",
    "is-valid-domain": "0.0.6",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7",
    "pino": "^5.9.0",
    "prettier": "^1.15.3",
    "task-container": "^1.0.6",
    "toml": "^2.3.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-swimlane": "^3.0.4",
    "typescript": "^3.2.1"
  }
}
